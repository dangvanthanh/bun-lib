{
  "name": "bundev",
  "module": "src/index.tsx",
  "type": "module",
  "scripts": {
    "db:push": "drizzle-kit push:sqlite",
    "db:studio": "drizzle-kit studio",
    "prepare": "panda codegen",
    "css:dev": "panda --watch",
    "dev": "concurrently \"bun run --watch src/index.tsx\" \"bun run css:dev\"",
    "bench": "bun run src/bench.ts",
    "lint": "bunx @biomejs/biome check ./src/**/*.{ts,tsx}",
    "format": "bunx @biomejs/biome format ./src/**/*.{ts,tsx} --write",
    "update": "bunx npm-check-updates --interactive --format group"
  },
  "dependencies": {
    "@elysiajs/cors": "^0.7.2",
    "@elysiajs/html": "^0.7.3",
    "@elysiajs/static": "^0.7.1",
    "@kitajs/html": "^3.0.10",
    "@kitajs/ts-html-plugin": "^1.3.3",
    "@libsql/client": "^0.3.6",
    "better-sqlite3": "^9.2.2",
    "drizzle-orm": "^0.29.1",
    "elysia": "^0.7.30"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@flydotio/dockerfile": "^0.4.11",
    "@pandacss/dev": "^0.21.0",
    "@types/react": "^18.2.45",
    "bun-types": "^1.0.17",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.6",
    "mitata": "^0.1.6"
  }
}
